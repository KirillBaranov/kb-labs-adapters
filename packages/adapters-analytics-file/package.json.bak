{
  "name": "@kb-labs/adapters-analytics-file",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "File-based analytics adapter for KB Labs platform (writes JSONL to .kb/analytics/buffer)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests"
  },
  "dependencies": {
    "@kb-labs/core-platform": "link:../../../kb-labs-core/packages/core-platform",
    "fs-extra": "^11.0.0",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@types/node": "^24.3.3",
    "@types/fs-extra": "^11.0.0",
    "eslint": "^9",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.11.0"
}
